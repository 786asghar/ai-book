{
  "version": 2,
  "builds": [
    {
      "src": "my-website/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "my-website/build",
        "rootDir": "my-website",
        "buildCommand": "npm install && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}